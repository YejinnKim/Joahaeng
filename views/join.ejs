<!-- Contact Start -->
<div class="container-xxl py-5">
    <div class="container">
        <div class="row g-4">
            <div class="wow fadeInUp" data-wow-delay="0.5s">
                <form>
                    <div class="row g-3 justify-content-center">
                        <div class="col-md-10">
                            <div class="form-floating">
                                <input type="text" class="form-control" id="userid" placeholder="ID">
                                <label for="id">ID</label>
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-floating">
                                <input type="password" class="form-control" id="userpw" placeholder="Password">
                                <label for="password">Password</label>
                            </div>
                        </div>
                        <div class="col-md-5">
                            <div class="form-floating">
                                <input type="password" class="form-control" id="userpwck" placeholder="Password Check">
                                <label for="password">Password Check</label>
                            </div>
                        </div>
                        <div class="col-md-10">
                            <div class="form-floating">
                                <input type="text" class="form-control" id="username" placeholder="name">
                                <label for="name">name</label>
                            </div>
                        </div>
                        <div class="col-md-10">
                            <div class="form-floating">
                                <input type="tel" class="form-control" id="userphone" placeholder="phone">
                                <label for="phone">phone</label>
                            </div>
                        </div>
                        <div class="col-md-10">
                            <input type="button" class="btn btn-primary w-100 py-3" value="Join" onclick="join();">
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<!-- Contact End -->